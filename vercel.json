{
  "buildCommand": "npm run build:frontend",
  "outputDirectory": "apps/frontend/dist",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/frontend/dist/$1"
    }
  ]
}
